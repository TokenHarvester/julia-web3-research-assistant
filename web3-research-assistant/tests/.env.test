# .env.test
NODE_ENV=test
JULIAOS_API_URL=https://mock-api.juliaos.com
JULIAOS_API_KEY=test_api_key_12345

# Mock blockchain endpoints
SOLANA_RPC_URL=https://mock-solana-rpc.com
