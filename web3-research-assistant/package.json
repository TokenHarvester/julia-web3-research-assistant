{
  "name": "web3-research-assistant",
  "version": "1.0.0",
  "description": "AI-powered Web3 project research assistant built with JuliaOS",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:unit": "cross-env NODE_ENV=test jest tests/unit",
